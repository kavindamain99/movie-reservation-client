iePayment/PaymentDetails.js","original":{"line":73,"column":13}},{"generated":{"line":109,"column":4},"source":"components/moviePayment/PaymentDetails.js","original":{"line":70,"column":12}},{"generated":{"line":109,"column":5},"source":"components/moviePayment/PaymentDetails.js","original":{"line":42,"column":8}},{"generated":{"line":109,"column":6},"source":"components/moviePayment/PaymentDetails.js","original":{"line":41,"column":4}},{"generated":{"line":110,"column":0},"source":"components/moviePayment/PaymentDetails.js","original":{"line":89,"column":7}},{"generated":{"line":112,"column":15},"source":"components/moviePayment/PaymentDetails.js","original":{"line":90,"column":19},"name":"PaymentDetails"}],"sources":{"components/moviePayment/PaymentDetails.js":" \r\nimport React, { useState } from \"react\";\r\nimport Axios from \"axios\";\r\nimport \"./Payment.css\";\r\n \r\n \r\n\r\nfunction PaymentDetails(){\r\n    const [name, setName] = useState(\"\");\r\n    const [phonenumber, setPhoneNumber] = useState(\"\");\r\n    const [email, setEmail] = useState(\"\");\r\n\r\n   const [paymentList, setPaymentList] = useState([]);\r\n    \r\n   const addPayment = () => {\r\n    Axios.post(\"http://localhost:8000/create\", {\r\n      name:name,\r\n      phonenumber:phonenumber,\r\n      email:email,\r\n    }).then(() => {\r\n       setPaymentList([\r\n         ...paymentList,\r\n         {\r\n          name:name,\r\n          phonenumber:phonenumber,\r\n          email:email,\r\n         },\r\n       ]);\r\n    });\r\n};  \r\n\r\nconst getPayment = () => {\r\n  Axios.get(\"http://localhost:8000/paymentdetails\").then((response) => {\r\n    //console.log(response.data);   \r\n      setPaymentList(response.data);\r\n\r\n    });\r\n};\r\n\r\n\r\n    return (\r\n        <div className=\"form-style-1\">\r\n          <div className=\"form-style-8\">\r\n            <label>Full Name:</label>\r\n            <input\r\n              type=\"text\"\r\n              onChange={(event) => {\r\n                setName(event.target.value);\r\n              }}\r\n            />\r\n            <label>Mobile:</label>\r\n            <input\r\n              type=\"text\"\r\n              onChange={(event) => {\r\n                setPhoneNumber(event.target.value);\r\n              }}\r\n            />\r\n            <label>Email:</label>\r\n            <input\r\n              type=\"text\"\r\n              onChange={(event) => {\r\n                setEmail(event.target.value);\r\n              }}\r\n            />\r\n             \r\n             \r\n            <button class=\"button\" onClick={addPayment}>Next</button>\r\n           \r\n            </div>\r\n            <div>\r\n            <link to=\"/\" onClick={getPayment}>Show </link>\r\n            \r\n            {paymentList.map((val, key) => {\r\n          return  (\r\n            <div>\r\n              <div>\r\n              <h3>Name: {val.name}</h3>\r\n              <h3>Mobile: {val.phonenumber}</h3>\r\n              <h3>Email: {val.email}</h3>\r\n              </div>\r\n            </div>\r\n\r\n          );\r\n        })}\r\n      </div>\r\n    </div>\r\n  );\r\n\r\n      } \r\n    export default PaymentDetails;\r\n\r\n"},"lineCount":null}},"error":null,"hash":"f54e64316a7c595154d88fbde1d00c9b","cacheData":{"env":{}}}